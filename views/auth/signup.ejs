<h3 class="authh1 authgroup">Signup form</h1>
<br>
<% include ../partials/flash %>

<form action="/signup" method="post">

  <div class="form-group authgroup">
    <label for="nickname">Nickname</label>
      <input class="form-control authinput" type="text" name="nickname" id="nickname" placeholder="Nickname">
  </div>

  <div class="form-group authgroup">
    <label for="email">Email</label>
      <input class="form-control authinput" type="email" name="email" id="email" placeholder="email">
  </div>

  <div class="form-group authgroup">
    <label for="password">Password</label>
      <input class="form-control authinput" type="password" name="password" id="password" placeholder="password">
  </div>
  <div class="btnwrap">
    <input class="btn btn-default authgroup btn-sm" type="submit">
  </div>
</form>
<br><br>
